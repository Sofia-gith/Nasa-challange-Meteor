{
  "name": "meu-outro-projeto",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:frontend": "cd web && npm install",
    "install:backend": "cd api && npm install",
    "build:frontend": "cd web && npm run build",
    "build:backend": "cd api && npm run build",
    "dev": "concurrently --names \"FRONT,API\" --prefix-colors \"blue,magenta\" \"npm run dev --prefix web\" \"npm run server --prefix api\""
  },
  "workspaces": [
    "web",
    "api"
  ],
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "axios": "^1.12.2"
  }
}
